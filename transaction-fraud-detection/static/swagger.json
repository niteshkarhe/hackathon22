{
  "openapi": "3.0.0",
  "info": {
    "description": "sean",
    "version": "1.0.0",
    "title": "transaction-fraud-detection-main",
    "license": {
      "name": "MIT",
      "url": "https://opensource.org/licenses/MIT"
    }
  },
  "servers": [
    {
      "url": "/"
    }
  ],
  "tags": [
    {
      "name": "Fraud Detection System",
      "description": "APIs for requesting and returning fraud transaction"
    }
  ],
  "paths": {
    "/fraud/predict": {
      "post": {
        "tags": [
          "Predict Fraud Transaction"
        ],
        "summary": "Predict Transaction is Fraud or not",
        "requestBody": {
          "description": "Predict Transaction is Fraud or not",
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/TransactionPostBody"
              }
            }
          }
        },
        "produces": [
          "application/json"
        ],
        "responses": {
          "201": {
            "description": "OK",
            "schema": {
              "$ref": "#/components/schemas/id"
            }
          },
          "400": {
            "description": "Failed. Bad post data."
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "id": {
        "properties": {
          "uuid": {
            "type": "string"
          }
        }
      },
      "TransactionPostBody": {
        "type": "object",
        "properties": {
          "step": {
            "type": "integer",
            "format": "integer"
          },
          "type": {
            "type": "string",
            "format": "number"
          },
          "amount": {
            "type": "number",
            "format": "number"
          },
          "nameOrig": {
            "type": "string",
            "format": "string"
          },
          "oldbalanceOrg": {
            "type": "number",
            "format": "number"
          },
          "newbalanceOrig": {
            "type": "number",
            "format": "number"
          },
          "nameDest": {
            "type": "string",
            "format": "string"
          },
          "oldbalanceDest": {
            "type": "number",
            "format": "number"
          },
          "newbalanceDest": {
            "type": "number",
            "format": "number"
          }
        }
      },
      "bookRequest": {
        "type": "object",
        "properties": {
          "title": {
            "type": "string",
            "format": "string"
          },
          "email": {
            "type": "string",
            "format": "email"
          },
          "timestamp": {
            "type": "string",
            "format": "number"
          }
        }
      },
      "bookRequests": {
        "type": "object",
        "properties": {
          "bookRequest": {
            "type": "object",
            "additionalProperties": {
              "$ref": "#/components/schemas/bookRequest"
            }
          }
        }
      }
    }
  }
}
